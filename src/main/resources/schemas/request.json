{
	"description": "Represents an incoming request.",
	"$id": "https://ofac.soundcu.com/request.schema.json",
 	"$schema": "https://json-schema.org/draft-07/schema",
 	"title": "Request Payload",
 	"type": "object",
	"properties": {
		"address": {
			"type": "string"
		},
		"birthdate": {
			"format": "date",
			"type": "string"
		},
		"city": {
			"type": "string"
		},
		"country": {
			"type": "string"
		},
		"id_number": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"postal_code": {
			"type": "string"
		},
		"ssn": {
			"type": "string"
		}
	},
	"required": [ "name" ]
}