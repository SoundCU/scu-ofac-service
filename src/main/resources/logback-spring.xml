<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<configuration>	
	<appender class="ch.qos.logback.core.ConsoleAppender" name="stdout">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>
	
	<root level="info">
		<appender-ref ref="stdout"/>
	</root>
</configuration>